pipeline {
    agent any
    stages {
        stage('Cleanup workspace'){
            steps {
                cleanWs()
            }
        }
        stage('Clone repo'){
            steps {
                git branch: 'main', url: 'https://github.com/piyanatnine/Final_SDTE'
            }
        }
        stage('Install dependencies'){
            steps {
                sh 'npm install'
            }
        }
    }
}